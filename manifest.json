{
  "manifest_version": 3,
  "name": "swipe-back",
  "version": "1.0.0",
  "description": "Go back to previous page by swiping left with two fingers on a trackpad.",
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "run_at": "document_end",
      "js": ["swipe-back.js"],
      "css": ["swipe-back.css"]
    }
  ]
}
